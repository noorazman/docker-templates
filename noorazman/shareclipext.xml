<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/noorazman/docker-templates/master/noorazman/shareclipext.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Productivity: Tools: Clipboard:</Category>
  <Name>noorazman-shareclipext</Name>
  <Description>
    [b]Description[/b][br]
    Shareclipext is a tool to share clipboards between different VM or machines.
  </Description>
  <Overview>
    Shareclipext is a tool to share clipboards between different VM or machines.
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/r/noorazman/shareclipext/</Registry>
  <GitHub>https://github.com/noorazman/shareclipext</GitHub>
  <Repository>noorazman/shareclipext</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3000</HostPort>
        <ContainerPort>3000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Icon></Icon>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams></ExtraParams>
</Containers>